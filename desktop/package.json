{
  "name": "ITSTelegram-v3",
  "productName": "ITSTelegram-v3",
  "version": "3.1.2",
  "description": "ITSumma Telegram Client",
  "main": "main.js",
  "scripts": {
    "start": "rm -rf .public && cp -rf ../public/. ./bundle/ && cp -rf ../dist/. ./bundle/  && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "prebuild": "rm -rf .public && cp -rf ../public/. ./bundle/ && cp -rf ../dist/. ./bundle/",
    "build:mac": "npm run prebuild && electron-forge make --arch=universal --platform=darwin",
    "build:lin":  "npm run prebuild && electron-forge make",
    "build:win": "npm run prebuild && deploy-win.bat",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "sergejsporysev",
    "email": "sergei.sporyshev@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "cli-truncate": "^4.0.0",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "node-static": "^0.7.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "electron": "28.0.0"
  }
}
